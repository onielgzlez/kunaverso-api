    "components": {
        "schemas": {
            "UserLoginRequest": {
                "title": "User Login request",
                "description": "User Login request body data",
                "required": [
                    "username",
                    "password"
                ],
                "properties": {
                    "username": {
                        "description": "Username or email",
                        "type": "string"
                    },
                    "password": {
                        "description": "Password",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "UserRegisterRequest": {
                "title": "User Register request",
                "description": "User Register request body data",
                "required": [
                    "name",
                    "username",
                    "email",
                    "password",
                    "password_confirmation"
                ],
                "properties": {
                    "name": {
                        "description": "Name",
                        "type": "string"
                    },
                    "lastnames": {
                        "description": "Lastnames",
                        "type": "string"
                    },
                    "username": {
                        "description": "Username",
                        "type": "string"
                    },
                    "email": {
                        "description": "Email",
                        "type": "string"
                    },
                    "phone": {
                        "description": "Cellphone",
                        "type": "string"
                    },
                    "password": {
                        "description": "Password",
                        "type": "string"
                    },
                    "password_confirmation": {
                        "description": "Password confirmation",
                        "type": "string"
                    },
                    "about": {
                        "description": "About",
                        "type": "string"
                    },
                    "brithday": {
                        "description": "Brithday",
                        "type": "string",
                        "format": "date"
                    }
                },
                "type": "object"
            },
            "UserPhotoRequest": {
                "title": "User Photo request",
                "description": "User Photo request body data",
                "required": [
                    "photo"
                ],
                "properties": {
                    "photo": {
                        "description": "image type jpg, jpeg, png",
                        "type": "string",
                        "format": "binary"
                    }
                },
                "type": "object"
            },
            "UserResource": {
                "title": "User Resource",
                "description": "User Resource",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Anonymous"
                    },
                    "lastnames": {
                        "type": "string",
                        "example": "Unknown Unknown"
                    },
                    "username": {
                        "type": "string",
                        "example": "anonymous"
                    },
                    "phone": {
                        "type": "string",
                        "example": "+xxxxxxxx"
                    },
                    "email": {
                        "type": "string",
                        "example": "anonymous@example.com"
                    },
                    "created": {
                        "type": "string",
                        "format": "date",
                        "example": "2023-10-19 16:19:48"
                    },
                    "birthday": {
                        "type": "string",
                        "format": "date",
                        "example": "2002-01-02"
                    },
                    "about": {
                        "type": "string",
                        "example": "I'am Anonymous born in January, etc."
                    },
                    "photo_path": {
                        "type": "string",
                        "example": "photos/32EXt5viOVQl1w6E9m11SF5YmEPc2qc6Pjewrmli.jpg"
                    },
                    "photo_path_url": {
                        "type": "string",
                        "example": "http://localhost/storage/photos/32EXt5viOVQl1w6E9m11SF5YmEPc2qc6Pjewrmli.jpg"
                    }
                },
                "type": "object"
            },
            "LoginResponse": {
                "title": "User Login response",
                "description": "User Login response",
                "properties": {
                    "message": {
                        "description": "Success message",
                        "type": "string",
                        "example": "You are successfully logged in."
                    },
                    "token": {
                        "description": "User token",
                        "type": "string",
                        "example": "2|UnyioeN35SPAbrByMflSiVr0ueCY74rCPBSIwr9y21108821"
                    }
                },
                "type": "object"
            },
            "LogoutResponse": {
                "title": "User Logout response",
                "description": "User Logout response",
                "properties": {
                    "message": {
                        "description": "Success message",
                        "type": "string",
                        "example": "You are successfully logged out."
                    }
                },
                "type": "object"
            },
            "PhotoDeletedResponse": {
                "title": "User Photo Delete response",
                "description": "User Photo Delete response",
                "properties": {
                    "message": {
                        "description": "User photo deleted",
                        "type": "string",
                        "example": "User photo deleted."
                    }
                },
                "type": "object"
            },
            "PhotoUpdatedResponse": {
                "title": "User Photo Updated response",
                "description": "User Photo Updated response",
                "properties": {
                    "message": {
                        "description": "User photo updated",
                        "type": "string",
                        "example": "User photo updated."
                    }
                },
                "type": "object"
            },
            "ProfileInformationUpdatedResponse": {
                "title": "User Profile information response",
                "description": "User Profile information response",
                "properties": {
                    "message": {
                        "description": "Success message",
                        "type": "string",
                        "example": "Profile information updated successfully."
                    }
                },
                "type": "object"
            },
            "RegisterResponse": {
                "title": "User Register response",
                "description": "User Register response",
                "properties": {
                    "message": {
                        "description": "Success message",
                        "type": "string",
                        "example": "Registration successful, we sent you and email to verify your email address."
                    },
                    "token": {
                        "description": "User token",
                        "type": "string",
                        "example": "2|UnyioeN35SPAbrByMflSiVr0ueCY74rCPBSIwr9y21108821"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "sanctum": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "Kunaverso",
            "description": "API Endpoints of Kunaverso"
        }
    ]
}